STARTRULES: +punc +indef-pns +per-pns +noun-phrases +main-verbs +secondary-verbs +negatives +conjunctions +interrogative-reversal +wh-question

RULENAME: punc
 FOCUS: .+?
 POINTS: 

RULENAME: indef-pns (A)
 FOCUS: pro:per|it*+pro:dem|it*+pro:dem|this+pro:dem|that*+det:dem|it*+det:dem|that*+det:dem|this+adv|that
 POINTS: A1
 FOCUS: qn|any+pro:indef|any*
 POINTS: A7
 FOCUS: qn|every+pro:indef|every*+adj|every*
 POINTS: A7
 FOCUS: qn|both+qn|few+qn|many+qn|each+qn|several+qn|most+adj|least+adj|last+adj|second+adj|third+adj|next+adj|first
 POINTS: A7
 FOCUS: qn|*+pro:indef|some*+det:num|*+pro:indef|one*+pro:indef|all
 POINTS: A3
 FOCUS: pro:indef|no*+(qn|no^pro:indef|one)
 POINTS: A4

# Z rules-   noun phrases
# these rules are used in conjunction with C or D to assign C or D points
# they do not assign their own points
# note- this should come after per-pns rule

RULENAME: noun-phrases (Z)
 FOCUS: n|*
 POINTS: Z
 FOCUS: n:*|*
 POINTS: Z

RULENAME: per-pns (B)
 FOCUS: pro:sub|I*+*pro:obj|me+pro:poss|mine+det:poss|my+pro:per|you*+det:poss|your+pro:poss|yours
 POINTS: B1
 FOCUS: pro:sub|he*+pro:obj|him+det:poss|his+pro:sub|she*+det:poss|her+pro:poss|hers+pro:obj|her
 POINTS: B2
 FOCUS: pro:sub|we+pro:obj|us+det:poss|our+pro:poss|ours+pro:sub|they*+pro:obj|them+det:poss|their+pro:dem|these+pro:dem|those+*pro:obj|us+det:dem|these+det:dem|those
